<?xml version="1.0" encoding="UTF-8" ?>
<table xmlns="https://github.com/dqinyuan/fakerpp"
       xsi:schemaLocation="https://github.com/dqinyuan/fakerpp
       https://raw.githubusercontent.com/dqinyuan/master/fakerpp.xsd"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       name="user_shop" ds="mysql">
    <joins>
        <rightjoin target="dt"/>
        <rightjoin target="shop"/>
        <leftjoin target="user"/>
    </joins>
    <col-families>
        <number>
            <random-double max-number-of-decimals="2" min="90" max="10000">
                <cols>
                    <col>amount</col>
                </cols>
            </random-double>
        </number>
    </col-families>
    <excludes>
        <exclude>id</exclude>
    </excludes>
</table>
